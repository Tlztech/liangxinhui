.all{
  width: 750rpx;
  font-size: 30rpx;
  color: #999;
}

/* itemborder */
.list{
  width: 690rpx;
  margin-left: 30rpx;
  border: 2rpx solid #ccc;
  border-radius: 20rpx;
}
.list .head{
  width: 100%;
  height: 100rpx;
  text-indent: 30rpx;
  line-height: 100rpx;
  border-bottom: 2rpx solid #ccc;
}
.list .body{
  padding: 30rpx 15rpx;
}
.list .body .itemborder{
  width: 660rpx;
  height: 70rpx;
  line-height: 70rpx;
  border: 2rpx solid #ccc;
  border-radius: 10rpx;
  display: flex;
  flex-direction: row;
}
.list .body .itemborder .left{
  width: 620rpx;
  height: 100%;
  text-indent: 25rpx;
}
.list .body .itemnom{
  width: 330rpx;
  height: 70rpx;
  line-height: 70rpx;
  border: 2rpx solid #ccc;
  margin-top: 40rpx;
  text-indent: 25rpx;
}
.list .body .item{
  width: 330rpx;
  height: 50rpx;
  margin-top: 20rpx;
  text-indent: 25rpx;
}
.list .bot{
  width: 330rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  background: orangered;
  border-radius: 10rpx;
  margin-top: 40rpx;
  margin-left: 180rpx;
}