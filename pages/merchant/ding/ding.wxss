.all{
  width: 750rpx;
  font-size: 30rpx;
  color: #999;
}

.list{
  width: 500rpx;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-left: 20rpx;
}
.list .nom{
  width: 140rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  text-align: center;
}
.list .sel{
  width: 140rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  color: orange;
  border-bottom: 8rpx solid orange;
  text-align: center;
}
.bar{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  background: #f4f4f4;
  display: flex;
  flex-direction: row;
}
.bar .item{
  height: 100%;
  font-size: 28rpx;
}

.w1{
  width: 150rpx;
}
.w2{
  width: 80rpx;
}
.w3{
  width: 120rpx;
}
.w4{
  width: 200rpx;
}

.merchant{
  width: 100%;
  height: 120rpx;
}
.merchant .items{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  border-bottom: 2rpx solid #ccc;
}
.merchant .items .item{
  color: #444;
  height: 100%;
  padding: 10rpx 10rpx;
  text-align: center;
}


